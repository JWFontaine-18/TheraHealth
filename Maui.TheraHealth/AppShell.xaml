<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Maui.TheraHealth.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Maui.TheraHealth"
    xmlns:views="clr-namespace:Maui.TheraHealth.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="Maui.TheraHealth">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Appointments"
        ContentTemplate="{DataTemplate views:AppointmentsView}"
        Route="Appointments" />

    

</Shell>
